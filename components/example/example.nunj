<div class="example">
  <p class="example__text">{{text}}</p>
  <div class="contacts-wrapper">
    <div class="contacts">
      {% for member in members %}
      <div class="contact-card" data-module="avatar">
        <img class="contact-card__avatar" src="{{member.avatar}}" alt="Avatar of {{member.name}}">
        <h6 class="contact-card__name">{{member.name}}</h6>
        <p class="contact-card__mail">{{member.email}}</p>
      </div>
      {% endfor %}
    </div>
  </div>

  <div class="swiper" data-module="swiper">
    <div class="swiper-container">
      <div class="swiper-wrapper">
        <div class="swiper-slide">
          <h3>Animal</h3>
          <img class="example__image" src="{{fakeImage_2}}" alt="{{image.alt}}">
        </div>
        <div class="swiper-slide">
          <h3>Random image</h3>
          <img class="example__image" src="{{fakeImage_1}}" alt="{{image.alt}}">
        </div>
        <div class="swiper-slide">
          <h3>BW Portrait</h3>
          <img class="example__image" src="{{image.url}}" alt="{{image.alt}}">
        </div>
      </div>
      <!-- If we need pagination -->
      <div class="swiper-pagination"></div>
      <!-- If we need navigation buttons -->
      <div class="swiper-button-prev"></div>
      <div class="swiper-button-next"></div>

      <!-- If we need scrollbar -->
      <div class="swiper-scrollbar"></div>
    </div>
  </div>
</div>

